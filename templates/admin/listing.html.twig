{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}

    {% block widgettop %}{% endblock %}
    <div class="m-portlet m-portlet--primary m-portlet--head-solid-bg m-portlet--head-sm" m-portlet="true" id="m_portlet_tools_2" style="">
        <div class="m-portlet__head">
            <div class="m-portlet__head-caption">
                <div class="m-portlet__head-title">
						<span class="m-portlet__head-icon">
							<i class="flaticon-users"></i>
						</span>
                    <h3 class="m-portlet__head-text">
                        {% block subheader %}{% endblock %}
                    </h3>
                </div>
            </div>
            <div class="m-portlet__head-tools">
                <ul class="m-portlet__nav">
                    {% block navitems %}{% endblock %}
                    <li class="m-portlet__nav-item">
                        <div class="m-input-icon m-input-icon--left">
                            <input type="text" class="form-control m-input" placeholder="Rechercher..." id="generalSearch">
                            <span class="m-input-icon__icon m-input-icon__icon--left">
                                <span><i class="la la-search"></i></span>
                            </span>
                        </div>
                    </li>

                    <li class="m-portlet__nav-item">
                        <a href="#" m-portlet-tool="fullscreen" class="m-portlet__nav-link m-portlet__nav-link--icon" aria-describedby="tooltip_9716uihgic"><i class="la la-expand"></i></a>
                        <div class="m-tooltip m-tooltip--portlet tooltip bs-tooltip-top" role="tooltip" id="tooltip_9716uihgic" aria-hidden="true" x-placement="top" style="position: absolute; will-change: transform; visibility: hidden; top: 0px; left: 0px; transform: translate3d(618px, -32px, 0px);">                            <div class="tooltip-arrow arrow" style="left: 48px;"></div>                            <div class="tooltip-inner">Fullscreen</div>                        </div></li>

                </ul>
            </div>
        </div>
        <div class="m-portlet__body" m-hidden-height="275" style="">
            <div class="row">

                <div class="col-lg-12">
                    {% block table %}{% endblock %}
                </div>
            </div>
        </div>
    </div>

{% endblock %}


{%  block javascripts %}
    {{ parent() }}
    <script src="/assets/vendors/block-ui/jquery.blockUI.js" type="text/javascript"></script>
    <script src="/assets/vendors/bootstrap-select/dist/js/bootstrap-select.js" type="text/javascript"></script>
    <script src="/assets/vendors/select2/dist/js/select2.full.js" type="text/javascript"></script>
    <script src="/assets/app/js/html-table.js" type="text/javascript"></script>
    <script src="/assets/app/js/sweetalert2.js" type="text/javascript"></script>
{% endblock %}


